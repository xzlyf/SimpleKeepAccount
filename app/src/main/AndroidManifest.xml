<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xz.ska">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo_max"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_max"
        android:supportsRtl="true"
        android:theme="@style/AppNoTitle">
        <activity android:name=".CategoryAddActivity"></activity>
        <activity android:name=".CategoryActivity" />
        <activity android:name=".MySelfActivity" />
        <activity android:name=".DetailActivity" />
        <activity
            android:name=".AddActivity"
            android:theme="@style/addActivity_theme" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>